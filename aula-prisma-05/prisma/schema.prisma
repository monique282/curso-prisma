generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id             Int      @id @default(autoincrement())
  label          String   @unique
  prince         Int
  description    String
  expirationDate DateTime
  createAt       DateTime @default(now())
  eatable        Boolean  @default(false)

  @@map("products")
}

